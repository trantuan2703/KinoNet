<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kinonet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_movie2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kinonet">
        <activity android:name=".code.View.Authen.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".code.View.Main.HomeActivity" />
        <activity android:name=".code.View.Authen.RegisterActivity" />
        <activity android:name=".code.View.Authen.LoginActivity">
        </activity>
    </application>

</manifest>